# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        l1=[]
        temp=head
        while  temp:
            l1.append(temp.val)
            temp=temp.next
        if l1==l1[::-1]:
            return True
        else:
            return False

        
space,time-o(n)
