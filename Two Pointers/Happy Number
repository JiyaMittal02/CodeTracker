//check the leetcode prem git for explanation , the time complexity of this code requires understanding

class Solution:
    def isHappy(self, n: int) -> bool:
         
        seen=set()
        if(n==1):
            return True
   
        while(n!=1):
            sum=0
            temp=n
            while(temp>0):
                digit=temp%10
                sum+=digit*digit
                temp=temp//10
            if sum in seen:
                return False
            seen.add(sum)
            n=sum
        return True
