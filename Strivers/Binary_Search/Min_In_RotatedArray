class Solution:
    def findMin(self, nums: List[int]) -> int:
        i=0
        j=len(nums)-1
        while i<j:
            mid=(i+j)//2
         
            if nums[mid]>nums[j]:   #if so then right half is where answer lies
                i=mid+1
            else:
                j=mid    # not mid-1 to keep in check the condition that mid can also be the potential answer
        return nums[i]
