BRUTE FORCE:

nums=[4,7,1,0]
#brute force
l1=[]
for i in range(len(nums)):
    j=i+1
    flag=True
    while j<len(nums):
        if nums[i]>nums[j]:
            j+=1

        else:
            flag=False
            break
        
    if flag:
        l1.append(nums[i])
print("Leaders are  ",l1)
TC-- O(N2)

OPTIMIZED VERSION: #right half mein se max nikal le agr current elt usse bdha hai toh automatically pure right half se badha hoga

nums=[4,7,1,0]
#brute force
l1=[]
i=0
righthalfmax=float('-inf')
for i in range(len(nums)-1,-1,-1):
    if nums[i]>righthalfmax:
        l1.append(nums[i])
        righthalfmax=nums[i]
l1.reverse()
print(l1)
